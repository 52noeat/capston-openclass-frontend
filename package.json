{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "cross-env nyc mocha-webpack --webpack-config build/webpack.base.conf.js --require test/setup.js test/**/*.spec.js"
  },
  "nyc": {
    "include": [
      "src/**/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "dependencies": {
    "@mdi/font": "^3.5.95",
    "axios": "^0.18.0",
    "babel-plugin-istanbul": "^5.1.1",
    "cross-env": "^5.2.0",
    "node-sass": "^4.11.0",
    "npm": "^6.9.0",
    "sass-loader": "^7.1.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-vue": "^0.8.1",
    "vue": "^2.6.6",
    "vue-analytics": "^5.16.4",
    "vue-meta": "^1.5.8",
    "vue-router": "^3.0.2",
    "vue-session": "^1.0.0",
    "vuetify": "^1.5.7",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-plugin-vue": "^5.0.0",
    "expect": "^24.7.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^14.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.3",
    "mocha-webpack": "^1.1.0",
    "nyc": "^13.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vee-validate": "^2.2.0",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
