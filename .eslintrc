{
    "parser": "vue-eslint-parser",
    "root": true,
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
        "no-unused-vars": 0,
        "max-len": [1, 200, 2, {"ignoreComments": true}],
        "vue/jsx-uses-vars": 2
    },
    "parserOptions": {
      "sourceType": "module",
        "ecmaVersion": 2018,
        "ecmaFeatures": {
            "globalReturn": false,
            "impliedStrict": false,
            "jsx": false
        }
    },
    "plugins": [
      "vue"
    ]    
}